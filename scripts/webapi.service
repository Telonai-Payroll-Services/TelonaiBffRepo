[Unit]
Description=Telonai BFF 

[Service]
WorkingDirectory=/var/www/dotnet
ExecStart=/usr/bin/dotnet TelonaiWebApi.dll
Restart=always
RestartSec=10
KillSignal=SIGINT
SyslogIdentifier=webapi
Environment=ASPNETCORE_Environment=dev
Environment=DOTNET_PRINT_TELEMETRY_MESSAGE=false
Environment=ASPNETCORE_URLS=http://*:81

[Install]
WantedBy=multi-user.target
